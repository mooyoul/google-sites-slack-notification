{
  "name": "google-sites-slack-notification",
  "version": "1.0.1",
  "description": "Receive Slack notification when Google Sites has updated.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mooyoul/google-sites-slack-notification.git"
  },
  "bugs": {
    "url": "https://github.com/mooyoul/google-sites-slack-notification/issues"
  },
  "homepage": "https://github.com/mooyoul/google-sites-slack-notification#readme",
  "scripts": {
    "start": "ts-node src/sync.ts",
    "build": "check-engine && rm -Rf dst && tsc && cd src && find . -name '*.json' -type f -exec cp {} ../dst/{} \\; && cd ..",
    "pack": "rm -f dst.zip && cp package.json dst/package.json && (cp package-lock.json dst/ || true) && (cp .npmrc dst/.npmrc || true) && cd dst && npm install --cache=../.npm --production && npm ls && zip -rqy ../dst.zip . && cd ..",
    "dynamodb:migrate": "ts-node tools/create-dynamodb-table.ts",
    "dynamodb:cleanup": "(test -e $TMPDIR/dynamodb-local.pid && pkill -SIGINT -P $(cat $TMPDIR/dynamodb-local.pid)); rm -f $TMPDIR/dynamodb-local.pid",
    "dynamodb:setup": "node $(npm bin)/local-dynamo --port 8000 & echo $! > $TMPDIR/dynamodb-local.pid; sleep 3",
    "pretest": "check-engine && rm -Rf dst && tsc -p ./tsconfig.test.json && cd src && find . -name '*.json' -type f -exec cp {} ../dst/{} \\; && cd .. && npm run dynamodb:cleanup && npm run dynamodb:setup",
    "test": "mocha --exit -t 20000 dst/**/__test__/**/*.js",
    "posttest": "npm run dynamodb:cleanup",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "deploy": "npm run build && npm run pack && serverless deploy",
    "deploy:stage": "npm run deploy -- -s stage",
    "deploy:prod": "npm run deploy -- -s prod",
    "info:stage": "sls info -s stage",
    "info:prod": "sls info -s prod"
  },
  "engines": {
    "node": "^8.10.0",
    "npm": "^6.10.3"
  },
  "author": "MooYeol Prescott Lee <mooyoul@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/cheerio": "0.22.13",
    "@types/debug": "4.1.5",
    "@types/html-to-text": "^1.4.31",
    "@types/jsdom": "12.2.4",
    "@types/lodash": "4.14.141",
    "@types/node": "8.10.54",
    "@vingle/serverless-tag-plugin": "^1.1.2",
    "@vingle/tslint-preset": "1.0.0",
    "check-engine": "1.8.1",
    "serverless": "1.53.0",
    "serverless-prune-plugin": "1.4.1",
    "tslint": "5.20.0",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "debug": "^4.1.1",
    "dynamo-types": "^2.7.1",
    "google-auth-library": "^5.3.0",
    "html-to-text": "^5.1.1",
    "jsdom": "^15.1.1",
    "lodash": "^4.17.15"
  }
}
